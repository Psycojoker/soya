-load static

%html
  %head
    %title
      =object.title
    %script{src: "{% static 'js/jquery.min.js' %}"}
    %script{src: "{% static 'mediaelementjs/mediaelement-and-player.min.js' %}"}
    %link{rel: "stylesheet", href: "{% static 'mediaelementjs/mediaelementplayer.min.css' %}"}

    :javascript
      $(function() {
        $("video").mediaelementplayer();
      })

  %body{style: "margin 0; padding: 0; border: 0 none; margin 0"}
    .player{style: "margin 0; padding: 0; border: 0 none"}
      %video{src: "{{ MEDIA_URL }}videos/{{ object.file_name }}", width: "100%", height: "100%"}
